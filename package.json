{
  "name": "vue-vlidator",
  "description": "Validation library inspired by Laravel's Validator",
  "version": "0.0.1-beta-0",
  "author": "Chantouch Sek <chantouchsek.cs83@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chantouch/validatorjs"
  },
  "bugs": {
    "url": "https://github.com/Chantouch/validatorjs/issues?labels=bug&milestone=1&page=1&state=open"
  },
  "keywords": [
    "validatorjs",
    "validator.js",
    "data validation",
    "validator",
    "validate",
    "validation",
    "data",
    "laravel",
    "laravel-validator-for-js"
  ],
  "dependencies": {
    "date-fns": "2.0.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@vue/cli-service": "^4.4.4",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "browserify": "^13.0.1",
    "chai": "~3.5.0",
    "eslint": "5.8.0",
    "grunt": "1.1.0",
    "grunt-browserify": "^5.3.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-concat": "~1.0.1",
    "grunt-contrib-watch": "~1.0.0",
    "jit-grunt": "^0.10.0",
    "karma": "~1.1.0",
    "karma-browserify": "^5.0.5",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "~1.0.0",
    "karma-growl-notifications-reporter": "0.0.2",
    "karma-mocha": "~1.1.1",
    "karma-mocha-reporter": "2.2.5",
    "laravel-vue-form-validator": "^0.0.20-beta",
    "lodash": "^4.17.15",
    "mocha": "~2.5.3",
    "mocha-better-spec-reporter": "github:mikeerickson/mocha-better-spec-reporter",
    "node-sass": "^4.14.1",
    "pump": "3.0.0",
    "puppeteer": "1.9.0",
    "rimraf": "3.0.2",
    "sass-loader": "^8.0.2",
    "vue-router": "^3.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "pre-commit": "npm run lint:spec && npm run lint && npm run test",
    "build": "rimraf dist && grunt dist",
    "lint": "eslint \"./src/**/*.js\"",
    "lint:spec": "eslint \"./spec/**/*.js\" --rule 'no-redeclare: off'",
    "lint:fix": "eslint \"./src/**/*.js\" --fix",
    "test:node": "node node_modules/mocha/bin/mocha spec --slow 500 --reporter mocha-better-spec-reporter",
    "test:node-watch": "node node_modules/mocha/bin/mocha --watch spec --slow 500 --reporter mocha-better-spec-reporter",
    "test:browser": "grunt dist && node node_modules/karma/bin/karma start --single-run",
    "test": "npm run test:node && npm run test:browser"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  }
}
