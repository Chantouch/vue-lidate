{
  "name": "vue-vlidator",
  "version": "0.0.1-beta-13",
  "description": "Vue validator",
  "scripts": {
    "build:copy": "npm run build && bash build/copy.sh",
    "build": "npm run build:module && npm run build:browser",
    "build:module": "bili --format esm-min --format cjs-min",
    "build:browser": "bili --format umd,umd-min --module-name vue-vlidator",
    "prepublish": "npm run build"
  },
  "main": "dist/index.es.min.js",
  "module": "dist/index.es.min.js",
  "browser": "dist/index.umd.min.js",
  "cdn": "dist/vue-vlidator.min.js",
  "unpkg": "dist/index.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chantouch/vue-vlidator.git"
  },
  "keywords": [
    "vue",
    "validator",
    "validation",
    "validate",
    "form",
    "client-validation"
  ],
  "author": "Chantouch Sek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chantouch/vue-vlidator/issues"
  },
  "homepage": "https://github.com/Chantouch/vue-vlidator#readme",
  "dependencies": {
    "haye": "^2.0.2",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "bili": "^5.0.2",
    "lodash": "^4.17.15"
  },
  "peerDependencies": {
    "lodash": "*"
  }
}
